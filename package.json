{
    "name": "3red",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve desktop --port 3000 --proxy-config proxy.config.json",
        "start:mobile": "ng serve mobile --port 3001 --proxy-config proxy.config.json",
        "build": "ng build desktop --aot --prod",
        "build:mobile": "ng build mobile --aot --prod",
        "build:prod": "npm run build && npm run build:mobile",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dev:ssr": "ng run desktop:serve-ssr",
        "serve:ssr": "node dist/server/main.js",
        "build:ssr": "npm run build:prod && ng run desktop:server:production",
        "prerender": "ng run desktop:prerender"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.2.1",
        "@angular/common": "~11.2.1",
        "@angular/compiler": "~11.2.1",
        "@angular/core": "~11.2.1",
        "@angular/forms": "~11.2.1",
        "@angular/platform-browser": "~11.2.1",
        "@angular/platform-browser-dynamic": "~11.2.1",
        "@angular/platform-server": "^11.2.1",
        "@angular/router": "~11.2.1",
        "@nguniversal/express-engine": "^11.2.0",
        "codelyzer": "^6.0.1",
        "core-js": "^2.5.4",
        "domino": "^2.1.6",
        "express": "^4.15.2",
        "localstorage-polyfill": "^1.0.1",
        "moment": "^2.24.0",
        "rxjs": "~6.6.3",
        "rxjs-compat": "^6.5.2",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1102.2",
        "@angular-devkit/build-optimizer": "^0.1102.2",
        "@angular/cli": "~11.2.1",
        "@angular/compiler-cli": "~11.2.1",
        "@angular/language-service": "~11.2.1",
        "@nguniversal/builders": "^11.2.0",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "angular2-http-file-upload": "^1.0.8",
        "angular2-template-loader": "^0.6.2",
        "angular2-text-mask": "^9.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "postcss-scss": "^3.0.4",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.7"
    }
}
